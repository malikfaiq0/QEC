    ViewBag.Title = "CourseEvaluationQuiz";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">


            <div class="card bg-info text-white">
                <div class="card-body">

                    <center><h3>Quiz 1: Draw Software Process Model</h3></center>

                </div>
            </div>


            <div class="card bg-light text-white">
                <div class="card-body">

                    <div class="row">

                        <div class="col-md-3"></div>

                        <div class="col-md-4">
                            <p>R1 = Designing</p>
                        </div>

                        <div class="col-md-4">
                            <p>R2 = Relationships</p>
                        </div>

                        <div class="col-md-3"></div>

                    </div>

                    <div class="row">

                        <div class="col-md-3"></div>

                        <div class="col-md-4">
                            <p>R3 = Selection</p>
                        </div>

                        <div class="col-md-4">
                            <p>R4 = Explanation</p>
                        </div>

                        <div class="col-md-3"></div>

                    </div>

                    <div class="row">

                        <div class="col-md-3"></div>

                        <div class="col-md-4">
                            <p>R5 = Implementation</p>
                        </div>

                        <div class="col-md-6"></div>

                    </div>

                </div>
            </div>

            <div class="card bg-light text-white">
                <div class="card-body">

                    <div class="row">

                        @*<div class="col-md-3"></div>

                        <div class="col-md-4">
                            <b>Course Name: Computer Programming</b>
                        </div>

                        <div class="col-md-4">
                            <b>Semester: SP21</b>
                        </div>

                        <div class="col-md-3"></div>

                    </div>



                    <div class="row">

                        <div class="col-md-3"></div>

                        <div class="col-md-4">
                            <b>Weeks Spent: Week 11</b>
                        </div>*@



                    </div>



                </div>
            </div>

            <div class="table100 ver1 m-b-110">
                <table data-vertable="ver1">
                    <thead>
                        <tr class="row100 head">
                            <th>
                                @Html.DisplayNameFor(model => model.Student.SudentName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Student.StudentRegNum)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Course.CourseTitle)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.AssignmentNumber)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.QuizNumber)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Project)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.AssignMarks)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.QuizMarks)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ProjectMarks)
                            </th>

                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {

                            <tr class="row100">

                                <td>
                                    @Html.DisplayFor(modelItem => item.Student.SudentName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Student.StudentRegNum)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Course.CourseTitle)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.AssignmentNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.QuizNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Project)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.AssignMarks)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.QuizMarks)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProjectMarks)
                                </td>


                                @*<td>
                                    <center>
                                        @Html.ActionLink("StudentEvaluationDetails", "StudentEvaluationDetails", new { id = item.StudentSARID }, new { Styles = "color:Blue" })

                                    </center>
                                </td>*@
                            </tr>






                        }
                    </tbody>
                </table>
