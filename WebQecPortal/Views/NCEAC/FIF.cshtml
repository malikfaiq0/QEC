@model WebQecPortal.Models.FIF
@{
    ViewBag.Title = "FIF";
}
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">

        <div class="container contact">
            <div class="row">
                <div class="col-md-12">
                    <div class="enter-class-header">
                        <h2 class="text-white text-center pt-3">FIF (Faculty Information Form) Form</h2>
                    </div>
                </div>
            </div>
            <br>
            <div class="row">

                <div class="col-md-10">
                    <div class="contact-form">


                        <div class="form-group">
                            <div class="col-sm-10">

                                <input type="text" class="form-control" id="fname" placeholder="Name" name="fname">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <input type="text" value="@ViewBag.AcademicDegree" class="form-control" id="lname" placeholder="Academic Degree (s)" name="lname">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <input type="text" value="@ViewBag.award" class="form-control" id="lname" placeholder="Degree Awarding Institution (s)" name="lname">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <input type="text" value="@ViewBag.specialization" class="form-control" id="lname" placeholder="Subjects and Specialization" name="lname">
                            </div>
                        </div>
                        <label>A.Experience (in years)</label>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <input type="text" value="@ViewBag.exphighedu" class="form-control" id="lname" placeholder="Higher Education" name="lname">
                                <input type="text" value="@ViewBag.expindustry" class="form-control" id="lname" placeholder="Industry" name="lname">
                            </div>
                        </div>
                        <label>B.Publication Record</label>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <label>1.Journal Publications:</label>
                                <input type="text" value="@ViewBag.pubJnlinter" class="form-control" id="lname" placeholder="International" name="lname">
                                <input type="text" value="@ViewBag.pubJnlLocal" class="form-control" id="lname" placeholder="Local" name="lname">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <label>2.Conference Publications:</label>
                                <input type="text" value="@ViewBag.pubConinter" class="form-control" id="lname" placeholder="International" name="lname">
                                <input type="text" value="@ViewBag.pubConlocal" class="form-control" id="lname" placeholder="Local" name="lname">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <label>3.Books</label>
                                <input type="text"  value="@ViewBag.books" class="form-control" id="lname" placeholder="Books" name="lname">
                                <input type="text" value="@ViewBag.bookchp" class="form-control" id="lname" placeholder="Book Chapters" name="lname">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <label>4.Patents</label>
                                <input type="text" value="@ViewBag.patents" class="form-control" id="lname" placeholder="  " name="lname">

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <label>5.Tech Reports</label>
                                <input type="text" value="@ViewBag.reports" class="form-control" id="lname" placeholder="  " name="lname">

                            </div>
                        </div>



                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button type="submit" class="btn btn-primary"><a href="@Url.Action("FinancialBSCS","NCEAC")" style="color:white;">Next</a></button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-2">
                    @Html.ActionLink("AdminStructure", "AdminStructure", "NCEAC")
                    <br />

                    @Html.ActionLink("Admission", "Admission", "NCEAC")
                    <br />
                    @Html.ActionLink("AlumniBSCS", "AlumniBSCS", "NCEAC")
                    <br />
                    @Html.ActionLink("AlumniBSSE", "AlumniBSSE", "NCEAC")
                    <br />

                    @Html.ActionLink("CourseDescription", "CourseDescription", "NCEAC")
                    <br />
                    @Html.ActionLink("CourseLog", "CourseLog", "NCEAC")
                    <br />

                    @Html.ActionLink("CPMF", "CPMF", "NCEAC")
                    <br />

                    @Html.ActionLink("FacultyWorkload", "FacultyWorkload", "NCEAC")
                    <br />
                    @Html.ActionLink("FIF", "FIF", "NCEAC")
                    <br />
                    @Html.ActionLink("FinancialBSCS", "FinancialBSCS", "NCEAC")
                    <br />
                    @Html.ActionLink("FinancialBSSE", "FinancialBSSE", "NCEAC")
                    <br />
                    @Html.ActionLink("HECadministration", "HECadministration", "NCEAC")
                    <br />
                    @Html.ActionLink("PMPF", "PMPF", "NCEAC")
                    <br />
                    @Html.ActionLink("CRI", "CRI", "NCEAC")
                    <br />
                </div>

            </div>
        </div>
    </section>
    <!-- /.content -->
</div>

<!-- /.content-wrapper -->
<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
</aside>
<!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
