@model WebQecPortal.Models.Institution

@{
    ViewBag.Title = "PMPF";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">

        <div class="container contact">
            <div class="row">
                <div class="col-md-12">
                    <div class="enter-class-header">
                        <h2 class="text-white text-center pt-3">PMPF Form</h2>
                    </div>
                </div>
            </div>
            <br>
            <div class="row">

                <div class="col-md-10">
                    <div class="contact-form">

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>INSTITUTION:</p>
                                <input type="text" value="@ViewBag.Institution" class="form-control" id="fname" placeholder="" name="fname">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>
                                    PROGRAM (S) TO BE
                                    EVALUATED
                                    :
                                </p>
                                <input type="text" value="@ViewBag.prog" class="form-control" id="fname" placeholder="" name="fname">
                            </div>
                        </div>

                        <b>Process to monitor the implementation of courses to ensure the following important attributes</b>

                        <br>
                        <br>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>How far objectives of the program have been achieved?</p>
                                <textarea name="name"  class="form-control">@ViewBag.objective</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>How frequent program is reviewed and revised? What is process/procedure?</p>
                                <textarea name="name"  class="form-control">@ViewBag.review</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>Summary of Problems Solving Skills Developed by the Program</p>
                                <textarea name="name" class="form-control">@ViewBag.problem</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>Summary of technologies exposed to the students</p>
                                <textarea name="name" class="form-control">@ViewBag.tech</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>Assessment Standards</p>
                                <textarea name="name"  class="form-control">@ViewBag.assess</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>How far program correlates with the existing industrial trends? Give details</p>
                                <textarea name="name"  class="form-control">@ViewBag.coRelate</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-10">
                                <p>Application of emerging technologies</p>
                                <textarea name="name"  class="form-control">@ViewBag.application</textarea>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button type="submit" class="btn btn-success"><a href="@Url.Action("FacultyWorkload","NCEAC")" style="color:white;">Submit</a></button>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-2">
                    @Html.ActionLink("AdminStructure", "AdminStructure", "NCEAC")
                    <br />

                    @Html.ActionLink("Admission", "Admission", "NCEAC")
                    <br />
                    @Html.ActionLink("AlumniBSCS", "AlumniBSCS", "NCEAC")
                    <br />
                    @Html.ActionLink("AlumniBSSE", "AlumniBSSE", "NCEAC")
                    <br />

                    @Html.ActionLink("CourseDescription", "CourseDescription", "NCEAC")
                    <br />
                    @Html.ActionLink("CourseLog", "CourseLog", "NCEAC")
                    <br />

                    @Html.ActionLink("CPMF", "CPMF", "NCEAC")
                    <br />

                    @Html.ActionLink("FacultyWorkload", "FacultyWorkload", "NCEAC")
                    <br />
                    @Html.ActionLink("FIF", "FIF", "NCEAC")
                    <br />
                    @Html.ActionLink("FinancialBSCS", "FinancialBSCS", "NCEAC")
                    <br />
                    @Html.ActionLink("FinancialBSSE", "FinancialBSSE", "NCEAC")
                    <br />
                    @Html.ActionLink("HECadministration", "HECadministration", "NCEAC")
                    <br />
                    @Html.ActionLink("PMPF", "PMPF", "NCEAC")
                    <br />
                    @Html.ActionLink("CRI", "CRI", "NCEAC")
                    <br />
                </div>

            </div>
        </div>
    </section>
    <!-- /.content -->
</div>

<!-- /.content-wrapper -->
<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
</aside>
<!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
