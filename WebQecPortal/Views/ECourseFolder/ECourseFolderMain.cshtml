@*@model IEnumerable < WebQecPortal.Models.ECourseModel>*@
@{
    ViewBag.Title = "ECourseFolderMain";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">


            <div class="card bg-info text-white">
                <div class="card-body">

                    <center><h3>ECourse Folders</h3></center>

                </div>
            </div>

            <div class="row">

                <div class="col-md-3">

                </div>

                <div class="col-md-6">

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Department</label>
                        @Html.DropDownList("Departments", (IEnumerable<SelectListItem>)ViewBag.dep)
                    </div>

                </div>

            </div>

            <div class="row">

                <div class="col-md-3">

                </div>

                <div class="col-md-3">


                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Select Term</label>
                        @Html.DropDownList("Terms", (IEnumerable<SelectListItem>)ViewBag.Terms)
                    </div>

                </div>


                <div class="col-md-3">

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Program</label>
                        @Html.DropDownList("Programs", (IEnumerable<SelectListItem>)ViewBag.prog)
                    </div>



                </div>


            </div>

            <div class="row">

                <div class="col-md-3">

                </div>

                <div class="col-md-3">

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Course Title</label>
                        @Html.DropDownList("Courses", (IEnumerable<SelectListItem>)ViewBag.course)
                    </div>


                </div>


                <div class="col-md-3">

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Instructor Name</label>
                        @Html.DropDownList("Instructors", (IEnumerable<SelectListItem>)ViewBag.instructor)
                    </div>

                </div>


            </div>

            <hr>

            <div class="row">

                <div class="col-md-1">
                    <div class="checkbox">
                        <label><input type="checkbox" value="">  All </label>
                    </div>
                </div>

                <div class="col-md-9">

                </div>

                <div class="col-md-2">

                    <div class="form-group">
                        <select class="form-control" id="exampleFormControlSelect1">
                            <option>Complete</option>
                            <option>Incomplete</option>
                        </select>
                    </div>

                </div>


            </div>

            <div class="table100 ver1 m-b-110">
                <table data-vertable="ver1">
                    <thead>
                        <tr class="row100 head">
                            <th class="column100 column1" data-column="column1">Term</th>
                            <th class="column100 column2" data-column="column2">Courses</th>
                            <th class="column100 column3" data-column="column3">Program</th>
                            <th class="column100 column7" data-column="column7"> Instructor</th>
                            <th class="column100 column7" data-column="column7"> Status</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var val in Model)
                        {
                        <tr>
                            <td class="Vcenter">@val.Terms.Semester</td>
                            <td class="Vcenter">@val.Courses.CourseTitle</td>
                            <td class="Vcenter">@val.Programs.Description</td>
                            <td class="Vcenter">@val.Instructors.InstructorName</td>
                            <td class="Vcenter">@val.Statuses.FolderStatus</td>
                            <td>
                                <center>
                                   @*// <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>*@
                                </center>
                            </td>
                        </tr>
                        }

                        @*<tr class="row100">
                            <td class="column100 column0" data-column="column0"><input type="checkbox" name="name1" />&nbsp;</td>
                            <td class="column100 column1" data-column="column1">SP21</td>
                            <td class="column100 column2" data-column="column2">SCD</td>
                            <td class="column100 column3" data-column="column3">BSCS</td>
                            <td class="column100 column4" data-column="column4">M Tauseef, Aman Farooq</td>
                            <td class="column100 column3" data-column="column3">Incomplete</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="../student_evaluation_viewdetails.html" style="color:white;">View Report</a></button>
                                </center>
                            </td>
                        </tr>

                        <tr class="row100">
                            <td class="column100 column0" data-column="column0"><input type="checkbox" name="name1" />&nbsp;</td>
                            <td class="column100 column1" data-column="column1">SP21</td>
                            <td class="column100 column2" data-column="column2">SCD</td>
                            <td class="column100 column3" data-column="column3">BSCS</td>
                            <td class="column100 column4" data-column="column4">M Tauseef, Aman Farooq</td>
                            <td class="column100 column3" data-column="column3">Complete</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="../student_evaluation_viewdetails.html" style="color:white;">View Report</a></button>
                                </center>
                            </td>
                        </tr>

                        <tr class="row100">
                            <td class="column100 column0" data-column="column0"><input type="checkbox" name="name1" />&nbsp;</td>
                            <td class="column100 column1" data-column="column1">SP21</td>
                            <td class="column100 column2" data-column="column2">SCD</td>
                            <td class="column100 column3" data-column="column3">BSCS</td>
                            <td class="column100 column4" data-column="column4">M Tauseef, Aman Farooq</td>
                            <td class="column100 column3" data-column="column3">Incomplete</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="../student_evaluation_viewdetails.html" style="color:white;">View Report</a></button>
                                </center>
                            </td>
                        </tr>*@

                    </tbody>
                </table>

                <br>
                <div class="btn-group">
                    <button type="submit" class="btn btn-secondary" style="float: right;">Send Notification</button>
                </div>
                <div class="btn-group">
                    <button type="submit" class="btn btn-secondary" style="float: right;">Print</button>
                </div>
                <div class="btn-group">
                    <button type="submit" class="btn btn-secondary" style="float: right;">Download</button>
                </div>
            </div>


        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
