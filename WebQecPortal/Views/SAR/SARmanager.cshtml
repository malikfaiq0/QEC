@*@model IEnumerable<WebQecPortal.Models.SARDataModel>*@



@{
    ViewBag.Title = "SARmanager";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">


            <div class="card bg-info text-white">
                <div class="card-body">

                    <center><h3>SAR Manager</h3></center>

                </div>
            </div>

            <div class="row">

                <div class="col-md-2">

                </div>

                <div class="col-md-4">

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Department</label>
                        @Html.DropDownList("Departments", (IEnumerable<SelectListItem>)ViewBag.dep)
                    </div>


                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Select Term</label>
                        @Html.DropDownList("Terms", (IEnumerable<SelectListItem>)ViewBag.Terms)

                    </div>

                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Program</label>
                        @Html.DropDownList("Programs", (IEnumerable<SelectListItem>)ViewBag.prog)
                    </div>

                    <div class="form-group">

                        <label class="form-label" for="customFile">Upload SAR</label>
                        <input type="file" class="form-control" id="customFile" />

                    </div>

                    <div class="form-group">
                        <div class="btn-group" style="float:right;">
                            <button type="submit" class="btn btn-success">Upload</button>
                        </div>
                    </div>



                </div>

                <div class="col-md-1">

                </div>

                <div class="col-md-3">

                    <center>
                        <b>About SAR</b>
                        <p>Version 1.0</p>
                    </center>
                    <div class="card bg-light text-black" style="border:1px black solid;">
                        <div class="card-body">

                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>
                            <h5 style="color:black;">*  ______________</h5>

                        </div>
                    </div>

                </div>
            </div>

            <hr>

            <div class="form-inline">
                @Html.DropDownList("Terms", (IEnumerable<SelectListItem>)ViewBag.Terms)

                <span>&nbsp;&nbsp;</span>
                @Html.DropDownList("Programs", (IEnumerable<SelectListItem>)ViewBag.prog)
                <span>&nbsp;&nbsp;</span>
                @Html.DropDownList("Departments", (IEnumerable<SelectListItem>)ViewBag.dep)
                <span>&nbsp;&nbsp;</span>
                <button type="submit" class="btn btn-primary">
                    Search
                </button>
            </div>

            <br>

            <div class="table100 ver1 m-b-110">
                <table data-vertable="ver1">
                    <thead>
                        <tr class="row100 head">
                            <th class="column100 column1" data-column="column1">Term</th>
                            <th class="column100 column2" data-column="column2">Program</th>
                            <th class="column100 column3" data-column="column3">Course</th>
                            <th class="column100 column7" data-column="column7"></th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var item in Model)
                        {
                        <tr>
                            <td class="Vcenter">@item.Terms.Semester</td>
                            <td class="Vcenter">@item.Programs.Description</td>
                            <td class="Vcenter">@item.Courses.CourseTitle</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>
                                </center>
                            </td>
                        </tr>
                        }
                        <!--<tr class="row100">-->
                        @*<td class="column100 column1" data-column="column1">FA20</td>
                            <td class="column100 column2" data-column="column2">BSSE</td>
                            <td class="column100 column3" data-column="column3">SCD</td>*@
                        <!--<td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>
                                </center>
                            </td>
                        </tr>-->

                        @*<tr class="row100">
                            <td class="column100 column1" data-column="column1">FA20</td>
                            <td class="column100 column2" data-column="column2">BSSE</td>
                            <td class="column100 column3" data-column="column3">MAD</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>
                                </center>
                            </td>
                        </tr>

                        <tr class="row100">
                            <td class="column100 column1" data-column="column1">FA20</td>
                            <td class="column100 column2" data-column="column2">BSSE</td>
                            <td class="column100 column3" data-column="column3">SQE</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>
                                </center>
                            </td>
                        </tr>

                        <tr class="row100">
                            <td class="column100 column1" data-column="column1">FA20</td>
                            <td class="column100 column2" data-column="column2">BSSE</td>
                            <td class="column100 column3" data-column="column3">HCI</td>
                            <td>
                                <center>
                                    <button type="submit" class="btn btn-info"><a href="@Url.Action("StudentEvaluation","SAR")" style="color:white;">Download SAR</a></button>
                                </center>
                            </td>
                        </tr>*@


                    </tbody>
                </table>
            </div>





        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
    </section>
    <!-- /.content -->
</div>

<div>
    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Control sidebar content goes here -->
    </aside>
    <!-- /.control-sidebar -->
</div>
